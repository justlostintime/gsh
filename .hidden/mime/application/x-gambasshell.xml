<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
        <mime-type type="application/x-gambasshell">
                <sub-class-of type="application/x-executable"/>
                <sub-class-of type="text/plain"/>
                <comment>Gambas Shell</comment>
                <comment xml:lang="fr">Shell Gambas</comment>
        <comment xml:lang="nl">Gambas shell</comment>
                <magic priority="50">
                        <match value="#!/usr/bin/env gsh" type="string" offset="0"/>
                        <match value="#!/usr/bin/gsh" type="string" offset="0"/>
                </magic>
                <glob pattern="*.gsh"/>
        </mime-type>
</mime-info>
