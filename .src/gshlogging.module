' Gambas module file

Public Sub _Call(logname As String, msg As String)

  Dim sfile As File

  Try sfile = Open logname For Write Append
  If Error Then
    Error "Unable to open log file "; logname
  Endif
  Try Print #sfile, Format(Now, "yyyy/mm/dd hh:nn:ss.uu");; msg
  Try Flush #sfile
  Try Close sfile

End

